{
  "name": "express-boilerplate-mysql",
  "description": "express boilerplate with mysql",
  "version": "1.0.0",
  "private": false,
  "license": "MIT",
  "bin": "src/bin/www.js",
  "main": "src/app.js",
  "author": {
    "name": "jhwmon",
    "email": "jmonaco88@gmail.com"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "keywords": [
    "api",
    "boilerplate",
    "express",
    "eslint",
    "jest",
    "joi",
    "mysql",
    "node",
    "node.js",
    "passport",
    "rest",
    "sequelize",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jhwmon/express-boilerplate-mysql"
  },
  "scripts": {
    "start": "npm run prod",
    "prod": "cross-env NODE_ENV=production npm-run-all build server",
    "build": "npm-run-all clean transpile",
    "clean": "rimraf build",
    "transpile": "babel src --out-dir build",
    "server": "node ./build/bin/www",
    "start:dev": "cross-env NODE_ENV=development nodemon -L --inspect=0.0.0.0 --exec ./node_modules/.bin/babel-node src/bin/www",
    "lint": "eslint -c './.eslintrc' --ignore-path .gitignore --ext .js .",
    "lint-and-fix": "eslint --ignore-path .gitignore --ext .js --fix .",
    "lint-all": "eslint .",
    "test": "cross-env NODE_ENV=test jest --verbose",
    "prettier": "prettier --check **/*.js",
    "prettier:fix": "prettier --write **/*.js"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "bcryptjs": "^2.4.3",
    "clean-deep": "^3.4.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "core-js": "^3.29.0",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "helmet": "^6.0.1",
    "joi": "^17.8.3",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "mysql2": "^3.1.2",
    "nanoid": "^4.0.1",
    "npm-run-all": "^4.1.5",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "qs": "^6.11.0",
    "redis": "^4.6.5",
    "regenerator-runtime": "^0.13.11",
    "sequelize": "^6.29.0",
    "slugify": "^1.6.5",
    "swagger-ui-express": "^4.6.2",
    "uuid": "^9.0.0",
    "validator": "^13.9.0",
    "xss-clean": "^0.1.1"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.0",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/passport": "^1.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "^13.7.12",
    "babel-plugin-module-resolver": "^5.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.35.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.4.3",
    "nodemon": "^2.0.21",
    "prettier": "^2.8.4",
    "rimraf": "^4.1.3",
    "supertest": "^6.3.3"
  }
}
